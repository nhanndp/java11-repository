package homework;

public class test {
	public static void main(String[] args) {
		int[] number = { 4, 7, 12, 18 };
		long[] factOfNumber = new long[4];
		int i = 0;
		for (i=0; i<=number.length; i++) {
			factOfNumber[i] *= number[i];
			break;
		}
		System.out.println("factorial of 4 is: " + factOfNumber[1]);
	}
}