package homework;

public class Ex05 {
	// Viết chương trình tìm số nguyên tố thứ 200 trong hệ thống số tự nhiên.

	public static void main(String[] args) {
		int n = 200;
		System.out.printf("200 prime numbers are: ", n);
	}

	public static boolean isPrimeNumber(int n) {
		if ( n < 2) {
			return false;
		} else {
			for (int i = 2; i <= n; i++) {
				if(n % i == 0) {
					return false;
				}
			}
		}
		return true;
	}
}
